<div class="form_one">
    <div class="label">
        <label for="doc">Doc:</label>
        </div>
        <div class="input">
        <input type="text" id="doc" name="doc" value="<%= busgasto.doc %>" placeholder="# Documento" autocomplete="off"/>
    </div>
     <div class="label">
        <label for="tipoPago">Tipo de Pago:</label>
        </div>
        <div class="input">
        <select name="tipoPago" id="tipoPago">
        <option value="0" 0 === busgasto.tipoPago ? "checked" : "">Seleccione</option>
            <option value="contado" "contado" === busgasto.tipoPago ? "checked" : "">Al Contado</option>
            <option value="credito" "credito" === busgasto.tipoPago ? "checked" : "">Cr√©dito</option>
        </select>
    </div>

    <div class="label">
        <label for="abonado">Efectivo Abonado:</label>
        </div>
        <div class="input">
        <input type="text" id="abonado" name="abonado" value="<%= busgasto.abonado %>" placeholder=" Abonado" autocomplete="off"/>
    </div>

    <div class="label">
        <label for="unidadId">Unidad:</label>
        </div>
        <div class="input">
        <select name="unidadId" id="unidadId">
            <% for (var i in unidads) { %>
                <% unidad = unidads[i]; %>
            <option value="<%= unidad.id %>" unidad.id === busgasto.unidadId ? "checked" : ""><%= unidad.codigo %>-<%= unidad.placa %></option>
            <% } %>
        </select>
    </div>

    <div class="label">
        <label for="proveedorId">Proveedor:</label>
        </div>
        <div class="input">
           <select name="proveedorId" id="proveedorId">
            <% for (var i in proveedors) { %>
                <% proveedor = proveedors[i]; %>
            <option value="<%= proveedor.id %>" proveedor.id === busgasto.proveedorId ? "checked" : ""><%= proveedor.nombre %></option>
            <% } %>
        </select>
    </div>

    
    <div class="label">
        <label for="productoId">Producto:</label>
        </div>
        <div class="input">
           <select name="reproductoId" id="reproductoId">
            <% for (var i in productos) { %>
                <% producto = productos[i]; %>
            <option value="<%= producto.id %>"  producto.id === busgasto.reproductoId ? "checked" : ""><%= producto.nombre %></option>
            <% } %>
        </select>
    </div>

    <div class="label">
        <label for="precio">Precio:</label>
        </div>
        <div class="input">
        <input type="text" id="precio" name="precio" value="" placeholder="Precio" autocomplete="off"/>
    </div>

      <div class="label">
        <label for="abonado">Cantidad:</label>
        </div>
        <div class="input">
        <input type="text" id="cant" name="cant" value="<%= busgasto.cant %>" placeholder="Cantidad" autocomplete="off"/><button class="buttonAdd addProducto" type="button">+</button>
    </div>


        <div class="container">
            <h1 class="text-center">Productos</h1>
            <hr>
            <div class="row">
             <div class="col-2">
                    
                        <h3>Pd</h3>
                    
                </div>
                <div class="col-6">
                    
                        <h3>Producto</h3>
                    
                </div>
                <div class="col-2">
                    
                        <h3>Precio</h3>
                    
                </div>
                <div class="col-1">
                    
                        <h3>Cant</h3>
                    
                </div>
                <div class="col-1">
                    
                      
                    
                </div>
            </div>
                <div class="items">
            </div>
            <div class="row">
             <div class="col-12">
                    
                        <h3><label class="total"></label></h3>
                    
                </div>
                    
        
            </div>        

    
     
    <a href="/goback"><input type="button" class="button_secundario" value="Cancelar"></a>
    <input class="button_primary enviarBus" type="button" value="Guardar">
</div>