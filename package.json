{
  "name": "etersacc-2022",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "NODE_ENV=testing mocha -R spec",
    "super": "supervisor ./bin/www",
    "migrate": "sequelize db:migrate --url sqlite://$(pwd)/etersac.sqlite",
    "seed": "sequelize db:seed:all --url sqlite://$(pwd)/etersac.sqlite",
    "migrate_win": "sequelize db:migrate --url sqlite://%cd%/etersac.sqlite",
    "seed_win": "sequelize db:seed:all --url sqlite://%cd%/etersac.sqlite",
    "migrate_env": "sequelize db:migrate --url $DATABASE_URL",
    "seed_env": "sequelize db:seed:all --url $DATABASE_URL"
  },
